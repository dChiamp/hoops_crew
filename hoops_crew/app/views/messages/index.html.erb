<h1>All Messages</h1>

<table id="messages">
  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td class="content"><%= message.content %></td>
        <!-- broke ass link -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Show', "#{current_user.id}/messages/#{@message.id}" %>
<%= link_to 'New message', new_message_path %>